<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.revature.LoginServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>loginsuccess</servlet-name>
    <servlet-class>com.revature.LoginSuccessServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>loginsuccess</servlet-name>
    <url-pattern>/loginsuccess</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>profile</servlet-name>
    <servlet-class>com.revature.ProfileServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>profile</servlet-name>
    <url-pattern>/profile</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>com.revature.LogoutServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>

</web-app>
